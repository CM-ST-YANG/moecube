{
  "name": "mycard",
  "description": "mycard",
  "author": "zh99998 <zh99998@gmail.com>",
  "homepage": "https://mycard.moe",
  "version": "3.0.0-dev.4",
  "repository": "github:mycard/mycard",
  "scripts": {
    "pack": "./node_modules/.bin/build --dir",
    "dist": "./node_modules/.bin/build",
    "release": "build",
    "electron": "electron .",
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "@angular/common": "latest",
    "@angular/compiler": "latest",
    "@angular/core": "latest",
    "@angular/forms": "latest",
    "@angular/http": "latest",
    "@angular/platform-browser": "latest",
    "@angular/platform-browser-dynamic": "latest",
    "@angular/router": "latest",
    "@angular/router-deprecated": "latest",
    "@angular/upgrade": "latest",
    "angular2-in-memory-web-api": "latest",
    "aria2": "latest",
    "bootstrap": "next",
    "core-js": "latest",
    "font-awesome": "latest",
    "ng2-translate": "2.5.0",
    "reflect-metadata": "latest",
    "rxjs": "latest",
    "systemjs": "latest",
    "tether": "latest",
    "zone.js": "latest"
  },
  "devDependencies": {
    "concurrently": "latest",
    "electron": "latest",
    "electron-builder": "latest",
    "lite-server": "latest",
    "typescript": "latest",
    "typings": "latest"
  },
  "build": {
    "productName": "MyCard",
    "appId": "com.mycard.mycard",
    "category": "public.app-category.social-networking",
    "win": {
      "iconUrl": "http://mycard.moe/logo.png",
      "remoteReleases": true
    },
    "mac": {
      "target": "dmg"
    },
    "dmg": {
      "title": "MyCard",
      "contents": [
        {
          "x": 448,
          "y": 344,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 192,
          "y": 344,
          "type": "file",
          "path": "mycard.app"
        }
      ]
    }
  }
}
